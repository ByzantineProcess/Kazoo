{
    "name": "Kazoo!",
    "version": "1",
    "manifest_version": 3,
    "description": "Kazoo homepage: https://github.com/ByzantineProcess/Kazoo",
    "icons": {
        "128": "icon/icon.png"
    },
    "action": {
        "default_title": "Kazoo!"
    },
    "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback"],
    "declarative_net_request" : {
        "rule_resources" : [{
          "id": "zapper",
          "enabled": true,
          "path": "kazoo/zapper.json"
        }]
      },
      "host_permissions": [
        "*://*.kahoot.it/*"
    ]
}